{
  "name": "@tableland/rigs",
  "version": "0.0.1",
  "description": "Tableland Rigs contract",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "typechain-types/index.js",
  "types": "typechain-types/index.d.ts",
  "files": [
    "contracts",
    "typechain-types/**/*.js?(.map)",
    "typechain-types/**/*.d.ts"
  ],
  "scripts": {
    "build": "npx tsc -p ./tsconfig.build.json",
    "clean": "rm -rf artifacts && rm -rf typechain-types && rm -rf cache && rm -rf coverage && rm coverage.json",
    "up": "npm install && npx hardhat compile && npm run build && hardhat node",
    "test": "hardhat coverage && istanbul check-coverage ./coverage.json --statements 100 --branches 60 --functions 100 --lines 100",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{ts,json,sol,md}' --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tablelandnetwork/rigs.git"
  },
  "keywords": [
    "tableland",
    "rigs",
    "ethereum"
  ],
  "license": "MIT AND Apache-2.0",
  "bugs": {
    "url": "https://github.com/tablelandnetwork/rigs/issues"
  },
  "homepage": "https://github.com/tablelandnetwork/rigs#readme",
  "devDependencies": {
    "@ethersproject/providers": "^5.6.8",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.1",
    "erc721a": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.6",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-dependency-compiler": "^1.1.3",
    "hardhat-gas-reporter": "^1.0.8",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.32",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.0-beta.1",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2"
  }
}
